#include<conio.h>
#include<stdio.h>

void main()
{
       int i,j,k,l;
       clrscr();

       for(i=1;i<=10;i++)
       {
	for(j=10;j>=i;j--)
	{
	 printf(" ");
	}
	for(k=1;k<=i;k++)
	{
	 printf("%d", k);
	}
       for(l=i;l>=1;l--)
       {
	printf("%d", l);
       }
       printf("\n");
       }

getch();
}
